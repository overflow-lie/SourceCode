--Data for one journey, collected by the TU. Contains start and stop data and fuel consumption.

JourneySummary1 ::= SEQUENCE {

    journeyId       CollectionId,

  startTrigger    TriggerType,

  startTime       TimeStamp,

  startOdometer   OdometerData,

  firstPosition   BasicPosition,

  --Distance driven between start and first position 4095 = 4095 or more

  firstPosDist    INTEGER (0..4095), 

  endTrigger      TriggerType,

  endTime         TimeStamp,

  endOdometer     OdometerData,

  endPosition     BasicPosition,

  avgFuelConsCl  AvgFuelConsumptionCl OPTIONAL, -- Not currently used

  --Note entered by user in the HMI, if available

  userNote        OCTET STRING (SIZE(0..63))  , -- not currently used

  tripType        TripType OPTIONAL, -- trip type assigned by user, not currently used

  ...

}

TripType ::= ENUMERATED {

   private   (0),

   business  (1)

  

}

-- (0..15)

--Data for one journey, based on received CAN signals

JourneySummary2 ::= SEQUENCE {

  

  journeyId            CollectionId OPTIONAL, --omitted if unknown or if a JourneySummary1 is also sent

  -- Indicates the units used for the data fields

  dashboardUnits        DashboardUnits,

  --Average fuel consumption in the unit shown on the dashboard.

  -- Note: Named fuel economy?in some specs

  --0-999 consumption * 10, 1000=unknown, 1001=initialization, 1002-1023 Reserved

  avgFuelConsx10       INTEGER(0..1023), 




  --Trip distance in the unit shown on the dashboard

  --0-16777214=dashboard value * 10, 16777215=unknown

  distancex10          INTEGER(0..16777215), 




  --Average speed in the unit shown on the dashboard

  --0-500=dashboard value, 510=unknown, 511=initialization

  avgSpd               INTEGER(0..511), 

  

  --Period for which ECO Start/Stop was active

  -- 0-16777214=value (unit?),  16777215=unknown

  ecoStopTimer         INTEGER(0.. 16777215), 




  --Hybrid vehicle data

  --Percentage of time the engine was off

  --0-100=percentage, 101=Unknown, 102=Initialisation, 103-127=Invalid

  engineOffPerc        INTEGER(0..127)  OPTIONAL,  --hybrid vehicles only  

  --Percentage of ?

  electricAssistPerc   INTEGER(0..127)  OPTIONAL,  --hybrid vehicles only. Currently this parameter will never be included or will not contain valid values (Can signals missing).

  -- Energy Recovered by hybrid module in terms of distance. 

  -- 0-16777214=value*10, 16777215=unknown

  energyRecoveredx10      INTEGER(0.. 16777215) OPTIONAL,  --hybrid vehicles only




  --Eco scores. 

  --0-9=invalid, 10-50=score*10, 51=unknown 52=initialization 53-63=invalid

  throttleEcoScorex10   INTEGER(0..63),   

  speedEcoScorex10     INTEGER(0..63),   

  brakeEcoScorex10     INTEGER(0..63),

  totalEcoScorex10     INTEGER(0..63),

  startTime TimeStamp OPTIONAL, --Start time or trip reset time

  ...

}




DashboardUnits ::= ENUMERATED {
   milespergallon,
   milesperhour,
   milesperliter,
   kmperliter,	
   literper100km,
   unknown
 }

