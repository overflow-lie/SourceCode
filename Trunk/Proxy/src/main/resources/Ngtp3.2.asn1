--A list of more or less generic error codes. Each service must describe if and when error codes are used.

ErrorCode ::= CHOICE {
	--消息顺序不正确 消息到达顺序在那里无法处理的
   incorrectSequence    [0]  NULL,   -- Message arrived out of sequence where it could not be processed. See also noOngoingService below.
	--ETC不正确 这个Tu应用已经处理了不同的EventCreationTime
   incorrectEct         [1]  NULL,   --TU application is already processing an event with a different EventCreationTime
	--状态不正确 
   incorrectState       [2]  NULL,   --The TU internal state prevented the message from being processed

   notAllowed           [3]  NULL,   --The TU is not allowed to process the request. This could happen if TSP is not following the specification or because of legislative reasons in a particular area.
	--数据校验错误
   invalidData           [4] NULL,  --Data received could not be interpreted.
	--数据格式错误
   invalidFormat         [5] NULL,  -- The format of received data was not accepted.
	--没有内存可以处理数据
   outOfMemory           [6] INTEGER (0..65535),  --The message is too large to be processed or there is not enough space to store the content, e.g. if the country database is full. The supplied data is defined per application.
	--没有持续的服务
   noOngoingService      [7] NULL,  --A special case of �incorrectSequence? where the service is currently idle. Used when a request which is not a start request arrives to an idling service.
	--当前车辆行动模式无法执行指令
   vehiclePowerModeNotCorrect [8] NULL,  --The request was not allowed to be processed due to the vehicle power mode.
	--执行的条件不正确,在没有其他更具体的错误时 使用此错误
   conditionsNotCorrect  [9] NULL,  --The conditions for executing the request was not correct (and no other more specific error code exist).
	--执行失败
   executionFailure      [10] NULL,  --The execution of the request failed. More detailed information about the execution failure can be added to genericError.
	--油面过低
  fuelLevelTooLow        [11] NULL,
	--电池电量太低
  batteryLevelTooLow     [12] NULL,

  runTimeExpiry          [13] NULL,
	--盗窃事件
  theftEvent             [14] NULL,
	--撞车事件
  crashEvent             [15] NULL,
	--超时
  timeout                [16] NULL,
	--车窗未关
  windowNotClosed        [17] NULL,
	--有危险警告
  hazardWarningOn        [18] NULL,
	--车辆未锁
  vehicleNotLocked       [19] NULL,
	--主动报警
  alarmActive            [20] NULL,
	--按下刹车踏板
  brakePedalPressed      [21] NULL,
	--车辆不固定
  vehicleNotStationary   [22] NULL,

  transmissionNotInPark  [23] NULL,  climateModeNotCorrect  [24] NULL,

  genericError         [255]  PrintableString (SIZE(1..63))




}
