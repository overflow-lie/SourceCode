

--filename:ngtp_services.asn

--

-- NGTP message format for NGTPServiceData 

--

-- For change history, see the Word document from which this file is exported

--

NGTP-Service-Data




DEFINITIONS

AUTOMATIC TAGS ::=

BEGIN

--========================================================================

-- NGTP业务数据

-- NGTPServiceData

--

--========================================================================

--Protocol versions used in the dispatcher part.

ProtocolVersionSingle8 ::= INTEGER (0..255)

serviceDataMajorVersion ProtocolVersionSingle8 ::= 3

serviceDataMinorVersion ProtocolVersionSingle8 ::= 2







NGTPServiceData ::= SEQUENCE {

  -- especially for uplink - to fulfill EU eCall requirements
	-- 对于上行消息 满足欧盟要求的紧急呼叫要求
  -- Generally set to FALSE if nothing else stated.
	-- testFlag 在不做说明时设置为false
  testFlag                BOOLEAN,

  



  --消息的类型 
  messageType                 ServiceMessageType,  

  --used if this message is an application acknowledge
  -- 应用application应答使用标记
  appAck                      Acknowledge OPTIONAL,

  

  -- can be set if an acknowledge is required on application level   
  -- 假如当前消息是APP消息,那么必填
  appAckRequired              BOOLEAN,

  
  --业务核心消息
  serviceDataCoreMessage      NGTPCore OPTIONAL,

  ... 

}
--消息的类型 
ServiceMessageType::= ENUMERATED{

   --Request/unsolicited message types

   dataRequest           (0),

   update                (1),

   startRequest          (2),

   terminateReq          (3),




   --Response message types

   dataResponse          (8),

   actionResponse        (9),

   acknowledge           (10)

} 
	--a(0..15)

NGTPCore ::= CHOICE {

  uplink      NGTPUplinkCore,

  downlink    NGTPDownlinkCore

}

--========================================================================

--

-- UPLINK CORE

--

--========================================================================



--上行核心消息
NGTPUplinkCore ::= SEQUENCE {
		--拓展的位置信息
  positionExtension       ExtendedPosition                          OPTIONAL,
		--基础的位置信息
  vehicleStatus           BasicVehicleStatus                        OPTIONAL,
		--拓展的车辆状态
  extendedVehicleStatus   ExtendedVehicleStatus                     OPTIONAL,
		--仪表板状态
  dashboardStatus         DashboardStatus                           OPTIONAL,
		--电动车状态
  electricVehicleStatus   ElectricVehicleStatus                     OPTIONAL,
		--气候状态
  climateStatus           ClimateStatus                             OPTIONAL,
		--TU状态
  tuStatus                TelematicUnitStatus                       OPTIONAL,
		--pearlChain(珍珠链??)
  pearlChain              BasicPearlChain                           OPTIONAL,
		--最后GPS时间戳
  lastGPSTimeStamp           TimeStamp                                 OPTIONAL,

  vin                     VIN                                       OPTIONAL,
		--导航信息
  navigationInformation   NavigationInformation                     OPTIONAL,
		--POI内存状态
  memoryStatus            POIMemoryStatus                           OPTIONAL,
		--车辆燃料类型
  vehiclePropulsionType   VehiclePropulsionType                     OPTIONAL,
		--碰撞数据
  crashData               CrashInformation                          OPTIONAL,
		--远程操作响应列表
  remoteOperationResponse RemoteOperationResponseList               OPTIONAL,
		--通用TEXT
  genericText             GenericText                               OPTIONAL,
		--通用参数 [255]
  genericParameters       SEQUENCE SIZE(0..255) OF GenericParameter OPTIONAL,
		--触发[15]
  trigger                  SEQUENCE SIZE(0..15) OF TriggerType       OPTIONAL,
		--故障告警[15]
  alerts                  SEQUENCE SIZE(0..15) OF AlertType         OPTIONAL,
		--轨迹点
  trackpoints             SEQUENCE SIZE(0..127)OF TrackPoint        OPTIONAL,
		--旅途情况
  journeys                SEQUENCE SIZE(0..127)OF JourneySummary    OPTIONAL,



		--MSD信息
  msdInfo                 MSDInformation                            OPTIONAL,
		--Debug信息
  debugInfo           	  SEQUENCE SIZE(0..10) OF DebugInfo		  OPTIONAL,
		--错误代码
  errorCodes              SEQUENCE SIZE(0..255) OF ErrorCode        OPTIONAL,

  

  ...

}

--========================================================================

--

-- DOWNLINK CORE

--

--========================================================================




NGTPDownlinkCore ::= SEQUENCE {
	--POI数据
  poiDataSet       SEQUENCE SIZE(0..15) OF PointOfInterest OPTIONAL,
	--远程操作请求列表
  remoteOperations RemoteOperationRequestList              OPTIONAL,
	--通用信息
  genericText      GenericText                             OPTIONAL,
	--Tu配置信息
  configuration    TuConfiguration                         OPTIONAL,

  genericParams   SEQUENCE SIZE(0..255) OF GenericParameter OPTIONAL,  --See the configuration chapter for limitations on the number and types of parameters allowed

  errorCodes              SEQUENCE SIZE(0..255) OF ErrorCode     OPTIONAL,




  ...

}

RemoteOperationRequestList ::= SEQUENCE {




   -- The Remote operation script version

   operationScriptVersion MajMinVersion16,
	--远程操作脚本版本 
   
	--触发条件
   executionTrigger RemoteOperationTrigger,

  

   -- The name of the remote operation.
   --这个名字是远程操作名称
   -- If this attribute names a well known remote operation then the 
   --假如可以确定动作,可以不写
   -- remoteOperations list may be empty.

   name RemoteOperationName OPTIONAL,

   

   -- The amount of time between executions (1/10 second).
  --在合适结束
   periodSecx10 INTEGER (0..1023) OPTIONAL,

  

   -- Number of times this operation may occur.  -1 indicates no limit.

   occurs INTEGER (-1..65534) OPTIONAL,

  

   -- List of Remote Operations to be executed. The list is ordered with the first operation in the list to be executed first.

   -- This list may be empty if a well known remote operation name has been specified
   --要执行远程操作的列表。列表被排序，以首先执行列表中的第一个操作。 
   -- 这个列表可能是空的，如果已指定一个众所周知的远程操作名称 
   remoteOperations SEQUENCE SIZE(0..31) OF RemoteOperationRequest,

   ...

}




RemoteOperationName ::= ENUMERATED {

--These are commands to be executed by the adressed service in the TU.The result is service dependant.

--Note: The list of services implementing a certain command is included for easier understanding and may not be fully correct or exhaustive. Not all commands are currently used

   rotNameEnd,     --End a service (eCall, bCall, SVT, TN)

   rotNameStart,     --Start a service (SVT)

   rotNamePause,     --Pause a service

   rotNameRestart,     --Restart a running or paused service

   rotNameConfigure,   --Configure a service

   rotNameGet,    --Request updated data (eCall, bCall, SVT HC,RVS) 

   rotNameGetPos,    --Request updated data including position, only for services where the position is optional (HC, RVS)   

   rotNameCleardown,  --Instruction for TU to expect CSC to hangup the voice call (eCall, bCall)

   rotNameCallback,  --Request service to call to CSC

   rotNameBlock,   --RVI block engine

   rotNameUnblock,   --RVI unblock engine

   --Note, rotNameEnd will end TN and all related services: SVT, RVI Blocking, UDD

   rotNameEndTnOnly,          --End TN but do not affect related services

   rotNameEndTnEndSvt,          --End TN and any ongoing tracking

   rotNameEndTnUnblock,         --End TN and unblock the vehicle

   rotNameDeactivate,          -- Take TU to deactivated unprovisioned state (may not be allowed by TU)

   rotNamePreactivate,         -- Take TU to preactivated unprovisioned state (may not be allowed by TU)

   rotNameActivate,            -- Take TU to activated unprovisioned state

   rotNameProvision,            -- Take TU to activated provisioned state

   rotNameHeat,                 -- Start heating

   rotNameVent,                 -- Start venting

   rotNameAuto                 -- Start auto operation (e.g. auto pre-conditioning).

}
--(0..255)

RemoteOperationTrigger ::= CHOICE

{

  -- no condition. do action immediately.
  --没有条件。马上做动作。
  now                      NULL,

  

  -- execute whenever is convenient for TU
  --当TU方便执行时
  at-convenience           NULL,




  -- periodically do action.
  --定期动作。
  periodic                 SEQUENCE

  {

    -- 1 sec (max one year). period == 0 means: execute once.
	--周期,  单位秒 .有效范围为 1到一年  365*24*60*60,0表示执行一次
    periodSec              INTEGER (0..31536000),
	--开始时间
    startAt                TimeStamp

  },




  -- execute as soon as possible but not later than the specified time

  no-later-than            TimeStamp,

  

  -- The contents of this trigger is OEM specific

  generic                  OCTET STRING (SIZE(0..1024)),




  -- do action after a distance have been travelled

  -- 100 m (100 - 100.000.000m)

  distanceMtrx100             INTEGER (1..1000000),




  -- do action when velocity exceeds a certain speed

  -- 1 km/h    (0 - 255 km/h)

  movementKmph             INTEGER (0..255),




  -- do action when vehicle has not been moving for this long

  -- 1 sec    (max 1 day)

  noMovementSec            INTEGER (0..86400),




  -- do action when engine starts

  engineOn                 NULL,




  -- do action when engine is turned off

  engineOff                NULL,




  -- do action when ignition is turned on

  ignitionOn                NULL,




  -- do action when ignition is turned off

  ignitionOff                NULL,




  -- do action when door status changes

  -- trigger on open/close or both

  doorStatusChange         ENUMERATED

  {

    open,

    close,

    both

  },




  -- Warnings to the driver except open doors

  vehicleTechnicalWarning  NULL,




  -- Vehicle error conditions

  vehicleError             NULL,




  -- do action when fuel level reaches limits

  -- 0-100% of total

  fuelLevelChangedAbovePerc    INTEGER (0..100),

  fuelLevelChangedBelowPerc    INTEGER (0..100),




  -- do action when temperature reaches limits

  -- -100 - 100 degree celcius

  internalTemperatureAboveDeg INTEGER (-100..100),

  internalTemperatureBelowDeg INTEGER (-100..100),




  externalTemperatureAboveDeg INTEGER (-100..100),

  externalTemperatureBelowDeg INTEGER (-100..100)




}

RemoteOperationRequest ::= SEQUENCE {




  -- The remote operation description
	--这个远程操作的详细说明
  operationDesc          SimpleRemoteOperation,




  -- Indicates that the execution of subsequent operations in a list should stop if the execution of this operation fails.
	-- 假如失败后是否继续后面步骤
  stopIfFail             BOOLEAN,

  

  -- Indicates if the previous command should complete before this operation is executed.
	--等待上一次命令
  waitForPreviousCommand BOOLEAN,

  

  -- Number of seconds before this operation times out
  --超时时间
  operationTimeoutSec       INTEGER (0..1023),




  -- The amount of time between executions (1/10 second).
  -- 结束时间
  periodSecx10            INTEGER (0..1023),

  

  -- Number of times this operation may occur.  -1 indicates no limit.

  occurs                 INTEGER (-1..65534),

  

  -- Amount of time to wait before this operation executes (1/10 second)
  --最初等待时间
  initialWaitTimeSecx10    INTEGER (0..1023),




   -- vehicle states required to be fulfilled when this operation is to be executed(moving, ign on/off, engine on/off, ...) 

   requiredVehicleStates VehicleStateType OPTIONAL,

   ...

     

}

VehicleStateType ::= ENUMERATED {
	--发动机开着并且移动中
  engine-on-moving         (0), -- Powermode=Running and VehicleSpeed>0
	--发动机开着,但未移动
  engine-on-park           (1), -- Powermode=Running and VehicleSpeed=0
	--车锁打开,但引擎关闭
  key-on-engine-off        (2), -- Powermode=Ignition or Acessory
	--?
  delayed-accessory        (3), -- Powermode=PostAcessory ?
	--钥匙关闭
  key-off                  (4), -- Powermode=Key inserted
	--未插要是
  key-removed              (5), -- Powermode=Key out
	--远程启动的发动机
  engine-on-remote-start   (6), -- Engine is started from remote



	--CanBus没有响应或者未连接CanBus
  unknown                  (15)  -- CAN bus asleep or TU is not connected to CAN

}
--(0..15)




SimpleRemoteOperation ::= SEQUENCE {

  

  -- The vehicle component this Operation addresses.
  -- 这个车辆操作的地址
  component              PrintableString (SIZE(0..256)),

  

  -- Type of operation to execute
  -- 执行的操作类型
  operationType          RemoteOperationType,

  

  -- The Components state the operationType returns true for when the response should be sent to the network.
  -- 在返回至网络是 应该有数据返回
  state                  UTF8String (SIZE(0..65535)) OPTIONAL,

  ...

}

RemoteOperationType ::= ENUMERATED {

   read                      (0),

   write                     (1),

   diagnostics-cancel        (2),

   read-async                (3),

   write-async               (4),

   diagnostics-read          (5),

   diagnostics-write         (6),

   diagnostics-track         (7),

   diagnostics-read-interval (8)

} 
--(0..31)

RemoteOperationResponseList ::= SEQUENCE {




	-- The Remote operation script version
	-- 这个远程操作使用的版本
    operationScriptVersion   MajMinVersion16,

    

    -- Number of times this operation has been executed (a.k.a hasOccurred)
	-- 当前操作被执行过的次数(或者已经发生的次数)
    timesExecuted            INTEGER(0..65535) OPTIONAL,

  

    -- Time when the request was last executed (successfully or not).
	-- 最后一次执行的时间(不管成功与否)
    lastExecuted             TimeStamp OPTIONAL,
	--返回结果 (数组)
    responses                SEQUENCE SIZE(1..31) OF RemoteOperationResponse,

    ...

}

RemoteOperationResponse ::= SEQUENCE {

  --The RemoteOperation
  --这个远程操作
		--对操作的返回(操作的说明)
  operationDesc      SimpleRemoteOperation,




  -- Number of times this operation has been executed (a.k.a hasOccurred)
  -- 该操作已经被执行的次数
  timesExecuted      INTEGER(0..65535),

  

  -- Time when the request was last executed (successfully or not).
	-- 最后一次执行的时间(不管成功与否)
  lastExecuted       TimeStamp,

  

  -- Error description in case of execution error.
  -- 在执行错误时的描述
  errorDesc          PrintableString (SIZE(1..256)) OPTIONAL,

  ...

}

MajMinVersion16 ::= SEQUENCE {

  majorVersion    ProtocolVersionSingle8,

  minorVersion    ProtocolVersionSingle8

}




TuConfiguration ::= SEQUENCE {

   --Configuration version, selected by TSP. 
   --配置的版本由TSP选择

   configVersion        TuConfigVersion,

   --List of countries and country specific configurations
	--国家 的具体配置(涉及有关应用的参数)
   countries            SEQUENCE SIZE(0..255) OF CountryConfigurationBlock,

   -- Everything which is not country specific will be configured through genericParameters. This includes which applications are provisioned. See Appendix D ?Generic Parameters

   ...

}

CountryConfigurationBlock ::= SEQUENCE

{

       --List of applications enabled in this country. If omitted, the global setting applies. Note: Currently not supported.
		--在这个国家的应用程序列表。如果省略该参数，则全局设置适用。注意：目前不支持。
    applications  SEQUENCE SIZE (0..31) OF INTEGER (0..255) OPTIONAL,




    validCountryCode   INTEGER(0..4095), --The MCC for which this Block applies

    applicationConfig  ApplicationConfiguration,

    --Addresses to use for external communication. Currently only phone numbers.

    addresses     SEQUENCE SIZE (0..15) OF AddressBlock,

    activated     BOOLEAN, --A possibility to configuring a Block without allowing it to be used.

    ...

}

--A list of application specific configurations which vary with the country in which the vehicle is located

ApplicationConfiguration::=SEQUENCE {

  eCall-requestCscNumber    BOOLEAN,  --Try to obtain a phone number from TSP before dialling private eCall or bCall. Note: An ecall/bCall number must always exist in the database for a private eCall/bCall to be attempted. (Currently not implemented. TU will always request a number)

  --Control if and how calls to PSAP through  network emergency call request can be used. Note that Great Britain, although the service center is public, is ?at least currently - treated as a �private?eCall country where �eCall?phone number shall be provisioned to 15999. �psapType?shall therefore be provisioned to �manualOnly?or �notAllowed?

  eCall-psapType   ENUMERATED {  

             notAllowed (0),   --An emergency call (manual or automatic) to PSAP shall never be made. Equivalent to not setting any of the psap numbers.

             manualOnly (1),   --PSAP may only be called if a human has manually authorized the call.

             autoAllowed (2),  --Automatic emergency calls to normal PSAP is allowed and should be used as backup if private CSC can not be reached.

             eUeCall (3)  --The country has infrastructure supporting eCall according to European standard with in-band modem. This setting is to be used also for Russia

             } ,
		--  (0..7)

  ...

}




AddressBlock ::= SEQUENCE

{

    addressId          INTEGER (0..255),  --Block id chosen by TSP. Context: This TuConfiguration block. Either references another AdressBlock from which data shall be taken or is used as a a reference by other blocks. (Depending on AdressType below)

    address            AddressType,      --Specifies details for a communication network and an address (normally a phone number) OR that data (TBD exactly what data?) shall be taken from an existing record with id=addressId

    addressUsage        ENUMERATED 

                       {

                           smsc,         --Smsc-number. Not supported by TU

                           destination,  --SMS destination number

                           ecall,        --service center voice call number for eCall

                           ecall-backup, --ecall backup number (Currently not used)

                           bcall,        --service center voice call number for bCall

                           psap,         --Number to PSAP for manual emergency call. When eCall-psapType is eUeCall, this number will be used only for test eCalls.

                           psap-auto    --Number to PSAP for automatic emergency call. If not set, the �psap?number will be used also for automatic emergency calls. (This adress is currently not used)

                       },
                       --(0..31)

   

    ...

}

AddressType ::= CHOICE

{

    absolute        CommunicationAddress,

    reference-id    NULL

}
-- (0..1)

CommunicationAddress ::= SEQUENCE

{

    networkID     NetworkType, --The type of network to which this address applies.Not supported

    validityRange ENUMERATED   --Not supported

      {

          unknown,

          international,

          national,

          reserved

      },

    address       Address,  --The actual address. Currently only phone numbers are supported

    ...

}

NetworkType ::= ENUMERATED

{

    unknown,

    isdn-speech,

    isdn-data,

    gsm-speech,

    gsm-sms,

    gsm-bs24,

    gsm-gprs,

    x-25,

    x-400-e-mail,

    internet-e-mail,

    reserved1,

    tcp-ip,

    udp-ip,

    fax-group-2-3,

    fax-group-4,

    gsm-bs-26,

    gsm-bs-26-udi,

    gsm-speech-uus,

    reserved (31)

} 
--(0..31)

Address ::= CHOICE

{

    --Address in bcd coded format. Each entry in the sequence shall be either: 

    -- A digit 0..9d for a phone number digit 0..9 or 

    -- 10d for ??or 11d for ??or 12d for ??
    address-bcd     SEQUENCE SIZE (0..20) OF INTEGER (0..15),

    address-ascii   OCTET STRING (SIZE(0..20))-- Not supported

}

--Command to end TN service and possibly also related services.

-- Note: The svtEnd and rviMobilize will be executed also if TN is not active
	



TNEndCmd ::= SEQUENCE {

   tnEnd         NULL,                  --No real meaning except to illustrate that TN, if active, will always end

   svtEnd        BOOLEAN DEFAULT TRUE,  -- If TRUE, Any ongoing tracking will be stopped.

   rviMobilize   BOOLEAN DEFAULT TRUE,  --If TRUE, the vehicle will become mobilized if immobilized or blocked.

   ...

}
--拓展的位置信息
ExtendedPosition ::= SEQUENCE {
	-- 高度
  altitudeMtr        INTEGER(-100..8091) OPTIONAL,

  headingDeg         INTEGER(0..359) OPTIONAL,
	--可靠性
  reliability     PositionReliability OPTIONAL,
  	--当前时间
  fixtime         TimeStamp OPTIONAL,
	--全球导航卫星系统速度 千米每小时
  gnssSpeedKmph   INTEGER(0..511) OPTIONAL,

  ...            

}

PositionReliability ::= SEQUENCE {

  fixType                     GnssFixType,

  

  --Map matching information

  insideDigitizedArea         BOOLEXT, -- isUnused if TU-internal GPS is used

  fullyDigitizedArea          BOOLEXT, -- isUnused if TU-internal GPS is used 

  matchedToDigitalMap         BOOLEXT, -- isUnused if TU-internal GPS is used 




  --Dead reckoning usage

  deadReckoning               BOOLEXT,

  --Dead reckoning calibration status

  drCalibrated                BOOLEXT,

  --Distance driven on dead reckoning, without any GNSS fix.

  -- 4095 means >=4095

  drDistanceMtr               INTEGER(0..4095),

  --Hdop=satellite geometry

  hdopx10                    INTEGER(0..127), --HDOP value multiplied by 10

  --Number of satellites used in the fix

  noOfSatellites              INTEGER(0..63),

  differentialGPSAvailable    BOOLEXT, -- �isUnused�in current HW

  ...

}

--Specify the fix type of the GNSS receiver. Note that the position may be 

--valid even if fixtype=none due to dead reckoning

GnssFixType::=ENUMERATED{

   none        (0),

   timeFix     (1),   --never used in the current HW

-- ASNPARSER DOES NOT SUPPORT MEMBERS STARTING WITH A NUMERIC VALUE

   fix2D       (2),

   fix3D       (3),

   fix3DHqFix     (4)  -- TRUE if the fix fulfills a number of internal criteria for being of high quality

}
--(0..15)

TrackPoint ::= SEQUENCE {
	--以实现
  --The reason for taking the trackpoint
  -- 行驶轨迹
  trigger          TriggerType,

  systemTime       TimeStamp,
	--集合ID
  collectionId     CollectionId, -- Note: SVT always uses 0
	--车辆的信息
  vehicle          VehicleTrackPointData OPTIONAL,
	--燃料信息
  fuel             FuelTrackPointData OPTIONAL,
	--位置信息
  gnss             FullPosition OPTIONAL,
	--网络信息(含定位信息)
  gsm              NetworkInformation OPTIONAL,
	--安全信息
  security         SecurityTrackPointData OPTIONAL,
	--拓展数据
  ext              TrackPointExtension,

  ...

}

--An id selected by TU for each journey to make it possible for TSP to match track points to the correct journey even if the time stamps for some reason are not withing start/stop time.
--跟踪点ID
CollectionId ::= INTEGER(0..255)

VehicleTrackPointData ::= SEQUENCE {
--里程表数据
  odometer             OdometerData,

  -- speed in 1/10 kmph. Max value=unknown.
  --速度单位为 1/10KMOG 4095为无效
  speedKmphx10        INTEGER (0..4095),
  --车辆当前状态
  vehicleState        VehicleStateType,

  ...

}

FuelTrackPointData ::= SEQUENCE {

  --Consumed fuel since trip start in 1/5ml or 200uL step. Max value=unknown.
  -- 消耗1/5或这200uL燃料经过的行程
  consFuelLtrx5000      INTEGER (0.. 16777215) OPTIONAL, 

  --Electrical consumption since trip start in 1/5 Wh or 200ukWh. Max value=unknown.
  -- 消耗 1/5 Wh or 200ukWh 电量经过的行程

  consElWhx5          INTEGER (0..16777215) OPTIONAL,

  --Electrical regeneration since trip start in 1/5 Wh or 200ukWh. Max value=unknown. 
  -- 消耗 1/5 Wh or 200ukWh 电量经过的行程

  regElWhx5           INTEGER (0..16777215) OPTIONAL 




}




-- Information about the current network to make a rough positioning possible

-- also without GNSS fix.

-- The RX-level is included because it may be required in legal discussions to 

-- prove that the car was outside coverage.

--

-- For security services and eCall, all known information should be included. 

-- If TU is not registered in any network it may include cell information from any

-- other base station seen




NetworkInformation ::= SEQUENCE {
	--网络注册状态
  status         NetworkRegistrationStatus,

  location       NetworkLocation OPTIONAL,

  

  --Rx level in percent of the maximum level

  rxLevelPerc    INTEGER(0..100)

}







NetworkRegistrationStatus ::= ENUMERATED {

   notRegistered,

   registered

} 
--(0..7)




NetworkLocation ::= CHOICE {
	--GSM网络定位
   gsmNwLocation  [0]  GsmNetworkLocation,
	--3G网络定位
   umtsNwLocation [1]  UmtsNetworkLocation,
	--4GLte网络定位
   lteNwLocation  [2]  LteNetworkLocation

}
--(0..31)




GsmNetworkLocation ::= SEQUENCE {
	--国家代码
  mcc            INTEGER(0..1023),   --country code
	--网络代码
  mnc            INTEGER(0..1023),   --network code
	--地点数据
  lac            INTEGER(0..65535),   --location area code
	--
  ci             INTEGER(0..65535),          --cell id
	--基站标识。
  bsic           INTEGER(0..63) OPTIONAL     --base station identity.    

}




UmtsNetworkLocation ::= SEQUENCE {

  mcc            INTEGER(0..1023),   --country code

  mnc            INTEGER(0..1023),   --network code

  lac            INTEGER(0..65535),   --location area code

  ci             INTEGER(0..4294967295)          --cell id

}







LteNetworkLocation ::= SEQUENCE {

